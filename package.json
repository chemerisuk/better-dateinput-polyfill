    "version": "gulp dist $npm_package_config_gulp && git add -A dist",
    "postversion": "git push && git push --tags",
    "publish": "gulp gh-pages $npm_package_config_gulp"
  },
  "browserslist": [
    "ChromeAndroid 30",
    "iOS 7",
    "IE 10"
